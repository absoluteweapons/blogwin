{
  "name": "blogwin",
  "version": "1.0.1",
  "description": "Jamie Evawin's personal blog.",
  "main": "index.md",
  "scripts": {
    "clean": "rm -rf _dist",
    "build": "export NODE_ENV=production && npx eleventy",
    "build-test": "npm run clean && export NODE_ENV=production && npx eleventy --serve",
    "develop": "export NODE_ENV=development && npx eleventy --serve",
    "develop-fresh": "npm run clean && npm run develop",
    "fresh-install": "rm -R node_modules && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamiegoodwin/blogwin.git"
  },
  "author": "Jamie Evawin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jamiegoodwin/blogwin/issues"
  },
  "homepage": "https://github.com/jamiegoodwin/blogwin#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-cache-assets": "^2.3.0",
    "@11ty/eleventy-img": "^1.0.0",
    "@tailwindcss/typography": "^0.5.0",
    "@toycode/markdown-it-class": "file:markdown-it-class",
    "autoprefixer": "^10.4.0",
    "clean-css": "^5.2.2",
    "cssnano": "^5.0.14",
    "dotenv": "^10.0.0",
    "html-minifier": "^4.0.0",
    "jsdom": "^19.0.0",
    "markdown-it": "^12.3.0",
    "markdown-it-responsive": "^0.1.0",
    "npm": "^8.3.0",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "posthtml": "^0.16.5",
    "posthtml-minify-classnames": "^0.3.0",
    "sharp": "^0.29.3",
    "tailwindcss": "^3.0.7",
    "terser": "^5.10.0"
  }
}
