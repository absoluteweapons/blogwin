{
  "name": "blogwin",
  "version": "1.0.1",
  "description": "Jamie Evawin's personal blog.",
  "main": "index.md",
  "scripts": {
    "clean": "rm -R _dist",
    "build": "export NODE_ENV=production && eleventy",
    "build-test": "export NODE_ENV=production && eleventy --serve",
    "develop": "export NODE_ENV=development && eleventy --serve",
    "develop-fresh": "npm run clean && npm run develop",
    "fresh-install": "rm -R node_modules && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamiegoodwin/blogwin.git"
  },
  "author": "Jamie Evawin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jamiegoodwin/blogwin/issues"
  },
  "homepage": "https://github.com/jamiegoodwin/blogwin#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-cache-assets": "^2.1.0",
    "@11ty/eleventy-img": "^0.8.2",
    "@gerhobbelt/markdown-it": "^12.0.4-53",
    "@gerhobbelt/markdown-it-responsive": "^0.1.1-3",
    "autoprefixer": "^10.3.1",
    "clean-css": "^4.2.3",
    "cssnano": "^5.0.7",
    "dotenv": "^8.2.0",
    "html-minifier": "^4.0.0",
    "jsdom": "^15.2.1",
    "markdown-it-class": "file:./markdown-it-class",
    "npm": "^7.7.5",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "posthtml-minify-classnames": "^0.2.3",
    "tailwindcss": "^2.1.x",
    "terser": "^5.7.1"
  }
}
